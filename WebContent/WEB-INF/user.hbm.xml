<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
<hibernate-mapping>  
<class name="org.crce.interns.view.User" table="User">  
          <id name="userName">  
          <generator class="assigned"></generator>  
          </id>  
          <property name="password"></property>  
          <property name="accountActive"></property>
          <property name="currentState"></property>
          <property name="lastLogin"></property>
          <property name="roleId"></property>
          <property name="createdDate"></property>
          <property name="modifiedDate"></property>
          <property name="modifiedBy"></property>  
</class>  
            
</hibernate-mapping>  